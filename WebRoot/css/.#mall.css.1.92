/*分类列表*/
.sortlist{border:1px #EDC889 solid;}
.sortlist .header{
	padding:0 10px;height:31px;line-height:31px;overflow:hidden;
	border-bottom:1px #EDC889 solid;background:#FCF7E6;color:#DD450C;
}
.sortlist dl{padding:2px 13px;line-height:30px;background:#fff;}
.sortlist dl a{color:#000}
