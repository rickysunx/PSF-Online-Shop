	margin-left:15px;display:inline-block; vertical-align:middle;
}
#mall_header .top .hold{position:absolute;bottom:0;right:5px;}
#mall_header .top .hold a{margin:0;}

/*mall布局<结束>*/


.rows_content:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.rows_content{zoom:1;clear:both}

.sidebar{float:left;width:185px;}
.r_content{float:right;width:800px;}
.v2_sidebar{float:left;width:219px}
.v2_r_content{float:right;width:766px;}

.rows{margin-bottom:5px;}

.top_banner_box:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.top_banner_box{zoom:1;clear:both}

.top_banner_box .shop_list_box{float:left;width:765px;}
.top_banner_box .shop_list,.top_banner_box .shop_list li{
	background:#F4F3EF;border-color:#F4F3EF;
}
.top_banner_box .active_banner_box{float:right;width:205px;}
.top_banner_box .active_banner_box .header{background-position:left -57px;}
/*mall布局<结束>*/

/*横幅条*/
#shortcut{
	background:url('/images/mall/default/mall_xbg.png') repeat-x;
	border-bottom-color:#141414;color:#fff;
}
#shortcut #shortcut_wrap{
	width:978px;padding:5px 0;
}
#shortcut .cutleft a,#shortcut .cutright .link a{color:#fff}
#shortcut .cutleft a:hover,#shortcut .cutright .link a:hover{color:#fff;text-decoration:underline}

#shortcut .morebtn .more{color:#fff;}

#shortcut .cutleft .icon em{background:url('/images/mall/default/mall.png') no-repeat 1px top;}
#shortcut .cutleft .icon_home em{background-position:-20px top;}
#shortcut .calc{color:#FFE8A0;}
#shortcut .calc:hover{color:#fff;}
.shortcut_welcome{color:#fff;}


.minishopbar{
	background:none;border-top-color:#A6A6A6;border-left-color:#727272;
	border-right-color:#2A2A2A;border-bottom-color:#000
}
.minishopbar .buy div{font-size:14px;color:#FF8400;font-weight:bold;width:120px}
.minishopbar .buy div b{color:#FEFEFE;}
.minishopbar .buy em{
	width:23px;height:23px;
	background:url('/images/mall/default/mall.png') no-repeat 0 -20px;margin-top:1px;
}


.minishopbar .buy .shopcar table {font-size:12px;font-weight:normal;}
.minishopbar .buy .shopcar td.scope,
.minishopbar .buy .shopcar td.price{color:#333}

.minishopbar .calc{font-weight:bold;font-size:14px;}
.minishopbar .calc em{
	width:16px;height:20px;vertical-align:-6px;_margin-top:3px;
	background:url('/images/mall/default/mall.png') no-repeat -31px -22px;
}

/*横幅条<结束>*/

/*导航条*/
.navgation{
	background:url('/images/mall/default/mall.png') no-repeat left -1370px;
	height:32px;line-height:32px;overflow:hidden;padding-bottom:5px;
}
.navgation ul{
	margin:0 -2px;padding-right:5px;float:left;
	background:url('/images/mall/default/mall.png') no-repeat #fff right -1410px;
}
.navgation li{
	padding:0 2px;float:left;font-size:14px;font-weight:bold;
	width:102px;height:32px;overflow:hidden;text-align:center;
	background:#fff;
}
.navgation li a{
	display:inline-block;color:#010101;width:100%;height:100%;overflow:hidden;
	background:url('/images/mall/default/mall.png') no-repeat left -1410px;
	line-height:33px;
}
.navgation .red{background-position:-106px -1410px}
.navgation .pink{background-position:-212px -1410px}
.navgation .green{background-position:-318px -1410px}
.navgation .blue{background-position:-424px -1410px}

.navgation .current a{background-position:left -1450px;color:#fff;}
.navgation .current .red{background-position:-106px -1450px}
.navgation .current .pink{background-position:-212px -1450px}
.navgation .current .green{background-position:-318px -1450px}
.navgation .current .blue{background-position:-424px -1450px}
/*导航条<结束>*/

/*搜索导航*/
.search_nav{height:40px;line-height:40px;background:#FCF7E6}
.search_nav .allsort,.search_nav .searchbox{float:left;margin-right:5px}
.search_nav .allsort{margin-right:0}
.search_nav .searchbox{padding-top:6px;padding-bottom:4px;}
.search_nav .link{float:left;color:#C0C0C0;height:100%;overflow:hidden;}
.search_nav .link a{color:#4C4C4C}
.search_nav .link a:hover{text-decoration:underline}
.search_nav .link span{color:#DD450C}

/*分类菜单*/
.allsort{
