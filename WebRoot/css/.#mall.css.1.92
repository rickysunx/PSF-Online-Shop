/*模拟FLASH广告--首页顶部*/
.slide_banner{
	width:800px;height:230px;overflow:hidden;
	position:relative;
}

.slide_banner img{
	position:absolute;width:100%;height:100%;
}
.slide_banner ul{
	position:absolute;width:100%;border-top:1px #B9B9B9 solid;
	height:30px;overflow:hidden;bottom:0;
}
.slide_banner li{
	border:1px #B9B9B9 solid;border-bottom:0;float:left;
	width:158px;height:29px;background:#000;cursor:pointer;
	text-align:center;color:#fff;line-height:29px;
}
.slide_banner li.current{background:#462A7A}
.slide_big_banner{height:338px;}

.roll_banner{
	width:800px;height:399px;overflow:hidden;
	position:relative;
}
.roll_banner ul{
	position:absolute;left:0;top:0;width:2700px;
}
.roll_banner ul li{
	float:left;width:800px;height:100%;overflow:hidden;
}
.roll_banner ul img{width:100%;height:100%;}
.roll_banner .control{
	position:absolute;width:100%;right:6px;bottom:8px;
	text-align:right;height:9px;overflow:hidden;font-size:1px;
}
.roll_banner .control a{
	display:inline-block;margin:0 4px;width:9px;height:9px;overflow:hidden;
	background:url('/images/mall/default/mall.png') no-repeat -55px -115px;
	vertical-align: top;
}
.roll_banner .control a.current{
	background-position:-40px -115px;
}

/*模拟FLASH广告--首页顶部<结束>*/

/*商品过滤器*/
.shop_filter:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.shop_filter{
	padding:7px 7px 2px;border:1px #CACACA solid;background:#fff;zoom:1;
}
.shop_filter h3{
	height:21px;line-height:21px;overflow:hidden;margin-bottom:8px;color:#9F3209;
}

.shop_filter .selected {
	background-color:#4598D2;color:#fff;
}

.shop_filter .item{padding:2px 0;border-top:1px #CACACA dashed;}
.shop_filter .item dl{
	padding:0 4px;border:1px #EDC889 solid;background:#FCF7E6;
}
.shop_filter .item .actions{float:right;width:100px;}
.shop_filter .item .actions .brandbtn{display:none;}

.shop_filter .item ul{
	margin-right:127px;_margin-right:124px;_border-right:1px FCF7E6 solid;
}
.shop_filter .item li{float:left;_display:inline;margin-right:6px;white-space:nowrap}
.shop_filter .item .operations{float:right;}
.shop_filter .item .operations .cancel{ text-decoration:underline}
.shop_filter .item .operations .okbtn{float:left;margin-right:5px;}
.shop_filter .item li input{
	float:left;height:21px;_height:19px;margin-top:2px;margin-bottom:-2px;
}
.shop_filter .item li label{float:left;}

.shop_filter dl{
	border-top:1px #CACACA dashed;padding:3px 5px;line-height:24px;
}
.shop_filter dt{float:left;width:80px;overflow:hidden;font-weight:bold;}
.shop_filter dd:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.shop_filter dd{
	margin-left:83px;_margin-left:80px;zoom:1;
}
.shop_filter dd li{float:left;_display:inline;margin:0 10px;white-space:nowrap}
.shop_filter dd .actions{
	padding:2px 0;line-height:20px;word-spacing:normal;float:right;text-align:right;
