}
.shop_filter .expandbtn{height:20px;}
.shop_filter .brandbtn,.shop_filter .okbtn{
	display:inline-block;height:18px;padding:0 5px 0 6px;line-height:19px;
	border:1px #D8BB55 solid;overflow:hidden;color:#DD450C;
	background:-moz-linear-gradient(#fff,#FCF7E7);
	background:-webkit-gradient(linear,0 0,0 100%,from(#fff),to(#FCF7E7));
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffffff, endColorstr=#FCF7E7,grandientT)
}
.shop_filter .okbtn{color:#333;}

/*商品过滤器<结束>*/

